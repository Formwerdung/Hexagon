{
  "name": "formwerdung/hexagon",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "A WordPress plugin which contains a library to individualize the WordPress backend for clients.",
  "homepage": "https://github.com/Formwerdung/Square",
  "authors": [
    {
      "name": "Formwerdung",
      "email": "mail@formwerdung.ch",
      "homepage": "https://github.com/Formwerdung"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "support": {
    "issues": "https://github.com/Formwerdung/Square/issues"
  },
  "require": {
    "php": ">=5.6.0",
    "composer/installers": "~1.0"
  },
  "extra": {
    "installer-paths": {
      "../../mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "../../plugins/{$name}/": ["type:wordpress-plugin"],
      "../../themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}
